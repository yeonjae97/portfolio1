<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chanyongyang.mapper.GrantMapper">
	<update id="update">
		update tbl_member set
		grantNo = #{grantNo},
		gradeNo = #{gradeNo}
		where id = #{id};
	</update>
	
</mapper>